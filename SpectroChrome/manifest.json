{
  "name": "Spectro",
  "description": "Remote Testing of Chemicals Sensors",
  "version": "0.4",
  "manifest_version": 3,
  "permissions": [
    "serial",
    "<all_urls>",
    "*://*/*",
    "fullscreen"
  ],
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": { "16": "/images/icon-16.png", "128": "/images/icon-128.png" }
}